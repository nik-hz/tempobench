    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: tempobench",
            "type": "debugpy",
            "request": "launch",
            "module": "src.tempobench",
            "args": [
                "--dataset_path",
                "src/tempobench/data/causal-done.jsonl",
                "--task",
                "causal"
            ]
        },
        {
            "name": "Python Debugger: datasets.py -c",
            "type": "debugpy",
            "request": "launch",
            "module": "src.tempobench.dataset",
            "console": "integratedTerminal",
            "args": [
                "t",
                "normal",
                "--file",
                "src/tempobench/data/trace-raw.jsonl",
                "--dump",
                "src/tempobench/data/trace-done.jsonl",
                "-s",
                "1"
            ]
        },
    ]
